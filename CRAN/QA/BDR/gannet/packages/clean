foreach f (tests tests-devel tests-clang tests-noLD tests-gcc-SAN tests-clang-ASAN tests-clang-UBSAN tests-ATLAS tests-MKL tests-OpenBLAS tests-Intel)
echo "cleaning $f"
rm -f ~/R/packages/$f/*/src/symbols.rds
rm -f ~/R/packages/$f/*/src/*.so
rm -f ~/R/packages/$f/*/src/*.mod
find ~/R/packages/$f -name \*.o -delete
find ~/R/packages/$f -name \*.a -delete
end
